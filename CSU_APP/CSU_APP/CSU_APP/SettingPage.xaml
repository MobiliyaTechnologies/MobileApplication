<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CSU_APP.SettingPage">
  <ContentPage.Content>
    <StackLayout Padding="0,20,0,0" Spacing="10">
      <ListView ItemsSource="{Binding cellData}" 
                x:Name="SettingList"
                SeparatorColor="Transparent">
        <ListView.ItemTemplate>
          <DataTemplate>
            <ImageCell ImageSource="{Binding thumbnailUrl}"
                       Text="{Binding title}" 
                       x:Name="itemClicked"
                       TextColor="#5B5857">

            </ImageCell>

          </DataTemplate>
        </ListView.ItemTemplate>
      </ListView>

      <ActivityIndicator
                           IsVisible="{Binding IsBusy}"
                           IsRunning="{Binding IsBusy}"
                           Color="Black" />

    </StackLayout>
  </ContentPage.Content>
</ContentPage>